type Query{
    user: User!
    getContributionStatus: ContributionStatus!
    hasCommitToday: Boolean!
}

type User{
    id: ID!
    name: String
    login: String
    avatarUrl: String
    email: String
}

type ContributionStatus{
    totalContributions: Int!
    days: [ContributionDay!]!
}

type ContributionDay{
    date: String!
    contributionCount: Int!
}